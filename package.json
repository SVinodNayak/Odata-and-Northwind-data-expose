{
  "name": "odata",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "dependencies": {
    "@sap-cloud-sdk/http-client": "^4.2.0",
    "@sap/cds": "^9",
    "express": "^4",
    "@sap-cloud-sdk/connectivity": "^4",
    "@sap-cloud-sdk/resilience": "^4"
  },
  "devDependencies": {
    "@cap-js/sqlite": "^2"
  },
  "scripts": {
    "start": "cds-serve"
  },
  "private": true,
  "cds": {
    "requires": {
      "Z301636_ODATAEXAMPLE_SRV": {
        "kind": "odata-v2",
        "model": "srv/external/Z301636_ODATAEXAMPLE_SRV",
          "credentials": {
            "destination": "remote",
            "path": "/sap/opu/odata/sap/Z301636_ODATAEXAMPLE_SRV"
        },
        "Northwind": {
      "kind": "odata-v2",
      "model": "srv/external/Northwind",
      "credentials": {
        "destination": "Northwind",
        "path": "/"
      },
        "csrf": true,
        "csrfInBatch": true
      }
    }
  }
}